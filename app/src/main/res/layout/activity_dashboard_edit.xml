<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dashboard.HTDashboardEditActivity"
    android:id="@+id/dashboardEditContainer">

    <com.sph.healthtrac.dashboard.HTCellDraggableListView
        android:id="@+id/listViewDashboardEdit"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@null"
        android:layout_marginTop="44dp"
        android:background="@color/ht_white"
        android:cacheColorHint="@color/transparent"
        android:listSelector="@color/transparent">
    </com.sph.healthtrac.dashboard.HTCellDraggableListView>

</RelativeLayout>
